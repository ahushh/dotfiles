Config { additionalFonts  = []
       , textOffset       = -1
       , bgColor          = "#000000"
       , fgColor          = "#e5e5e5"
       , alpha            = 255
       , iconRoot         = "."
       , iconOffset       = -1
       , position         = Top
       , lowerOnStart     = False
       , hideOnStart      = False
       , pickBroadest     = False
       , allDesktops      = True
       , overrideRedirect = True
       , persistent       = False
       , border           = NoBorder
       , borderColor      = "#990000"
       font = "xft:DejaVu Sans Mono:pixelsize=10",
       commands = [
                Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10,
                Run Memory ["-t","Mem: <usedratio>%"] 10,
                Run Date "%a %b %_d %H:%M" "date" 10,
                Run StdinReader,
                Run MPD ["-t", "<statei> <artist> - <title> <lapsed>/<length>", "--", "-P", ">>", "-Z", "><", "-S", "Ã—"] 10,
                Run CoreTemp ["-t", "Temp: <core0>C",
                              "-L", "40", "-H", "60",
                              "-l", "lightblue", "-n", "gray90", "-h", "red"] 50,
                Run Com "/home/user/bin/bat.sh" [] "bat" 60,
                Run BatteryP ["BAT0"]
                             ["-t", ": <watts><left>%/<timeleft>",
                             "-L", "10", "-H", "80", "-p", "3",
                             "--", "-O", "<fc=green>On</fc> - ", "-i", "",
                             "-L", "-15", "-H", "-5",
                             "-l", "red", "-m", "blue", "-h", "green"]
                             20
                ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "              %StdinReader% }{ %mpd% | %cpu% | %memory% | %coretemp% | %bat% %battery%    <fc=#ee9a00>%date%</fc>             "
       }
